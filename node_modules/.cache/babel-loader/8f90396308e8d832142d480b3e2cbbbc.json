{"ast":null,"code":"import TheHero from \"./components/TheHero.vue\";\nimport TheForm from \"./components/TheForm.vue\";\nimport TheCan from \"./components/TheCan.vue\";\nimport TheCant from \"./components/TheCant.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    TheHero,\n    TheForm,\n    TheCan,\n    TheCant\n  },\n\n  data() {\n    return {\n      userInfo: {},\n      userData: {},\n      sendingData: {}\n    };\n  },\n\n  methods: {\n    check(userInfo) {\n      this.userInfo = {\n        usedLangs: userInfo.usedLangs,\n        favoriteLang: userInfo.favoriteLang,\n        whyLearn: userInfo.whyLearn,\n        whyNot: userInfo.whyNot,\n        learnFirst: userInfo.learnFirst,\n        learnToday: userInfo.learnToday\n      };\n    },\n\n    checkData(userData) {\n      this.userData = {\n        firstName: userData.firstName,\n        lastName: userData.lastName,\n        email: userData.email,\n        job: userData.job,\n        code: userData.code,\n        nameIsValid: userData.nameIsValid,\n        lastIsValid: userData.lastIsValid,\n        emailIsValid: userData.emailIsValid\n      }, this.checkAll();\n    },\n\n    checkAll() {\n      let regexFirst = /^[A-Za-z]+$/;\n\n      if (!this.userData.firstName.match(regexFirst) && !document.querySelectorAll(\".invalid-info\")[0].classList.contains(\"invalid\")) {\n        document.querySelectorAll(\".invalid-info\")[0].classList.add(\"invalid\");\n      }\n\n      if (this.userData.firstName.match(regexFirst)) document.querySelectorAll(\".invalid-info\")[0].classList.remove(\"invalid\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVL,OADU;IAEVC,OAFU;IAGVC,MAHU;IAIVC;EAJU,CAFC;;EASbG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,QAAQ,EAAE,EAFL;MAGLC,WAAW,EAAE;IAHR,CAAP;EAKD,CAfY;;EAiBbC,OAAO,EAAE;IACPC,KAAK,CAACJ,QAAD,EAAW;MAC